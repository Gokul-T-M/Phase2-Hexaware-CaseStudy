@model ClientDirectoryApp.Models.ClientInfo
@{
    ViewData["Title"] = "Client Detail";
}


<h2>Client Details</h2>
<table class="table table-striped">
    <tr><th>ID</th><td>@Model.ClientId</td></tr>
    <tr><th>Name</th><td>@Model.CompanyName</td></tr>
    <tr><th>Email</th><td>@Model.Email</td></tr>
    <tr><th>Web URL</th><td>@Model.WebUrl</td></tr>
    <tr><th>Category</th><td>@Model.Category</td></tr>
    <tr><th>Standard</th><td>@Model.Standard</td></tr>
</table>