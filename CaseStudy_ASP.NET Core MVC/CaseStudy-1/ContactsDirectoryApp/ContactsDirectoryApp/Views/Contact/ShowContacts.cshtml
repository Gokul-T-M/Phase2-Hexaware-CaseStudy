@model List<ContactsDirectoryApp.Models.ContactInfo>

<h2>Contact List</h2>

<table border="1">
    <tr>
        <th>ContactId</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Company</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Designation</th>
    </tr>
    @foreach (var contact in Model)
    {
        <tr>
            <td>@contact.ContactId</td>
            <td>@contact.FirstName</td>
            <td>@contact.LastName</td>
            <td>@contact.CompanyName</td>
            <td>@contact.EmailId</td>
            <td>@contact.MobileNo</td>
            <td>@contact.Designation</td>
        </tr>
    }
</table>